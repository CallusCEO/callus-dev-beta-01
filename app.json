{
	"expo": {
		"name": "callus-dev-beta-01",
		"slug": "callus-dev-beta-01",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/Logo-Callus.jpg",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,

		"ios": {
			"supportsTablet": true
		},

		"splash": {
			"image": "./assets/images/Logo-Callus.jpg",
			"resizeMode": "contain",
			"backgroundColor": "#111"
		},

		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/Logo-Callus.jpg",
				"backgroundColor": "#111"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		}
	}
}
